
@article{
王爽赵会洋-399,
   Author = {王爽 and 赵会洋},
   Title = {LaTex排版中选项的设计研究},
   Journal = {电脑知识与技术},
   Volume = {15},
   Number = {03},
   Pages = {213-214+217},
   Note = {页数: 3},
   Abstract = {LaTex是国际公认的一个非常出色的排版工具,对各类科技工作者的研究发表起着非常重要的辅助作用。该文采用理论与实践相结合的方法研究了LaTex中选项的设计与使用方法。首先阐明了选项的使用步骤,最后通过综合实验对选项的作用进行了例证。本研究对灵活运用LaTex进行排版具有实际意义。},
   Keywords = {LaTex;排版;选项;文档类},
   Year = {2019} }




@article{
王爽赵会洋-403,
   Author = {王爽 and 赵会洋},
   Title = {基于程序员思维研究La Tex排版},
   Journal = {智能计算机与应用},
   Volume = {9},
   Number = {03},
   Pages = {199-200+205},
   Note = {页数: 3},
   Abstract = {LaTex是国际公认的一个非常出色的排版工具,对各类科技工作者的研究发表起着非常重要的辅助作用。不同于Word排版的所见即所得,LaTex是基于不同的命令实现排版的,因此对于初学者来说有一定的难度。本文基于程序员的思维,研究编程中的变量、函数等重要内容与LaTex排版的关系。这对快速学习与掌握LaTex具有重要的作用,对灵活运用LaTex进行排版具有实际意义。},
   Keywords = {LaTex;编程;排版;变量;函数},
   Year = {2019} }




@article{
王雪董博-409,
   Author = {王雪 and 董博},
   Title = {基于云计算平台实现LaTeX文稿在线采编},
   Journal = {软件导刊},
   Volume = {15},
   Number = {06},
   Pages = {5-7},
   Note = {页数: 3},
   Abstract = {LaTeX是一种基于Tex的文档排版系统,具有排版统一和良好的跨平台特性,可以在多种操作系统上实现相同的排版样式,尤其适合公式和参考文献的排版,是目前最流行的科技论文写作工具。但由于其命令繁多、环境配置过程复杂、版本多样,使得论文写作人员应用不便,望而却步。针对这一问题,提出了基于云计算平台实现LaTeX文稿可视化在线采编系统架构,可运用云计算平台进行文稿在线采编,在文稿撰写过程中为写作人员提供可行性建议。通过事例说明了其用法。},
   Keywords = {LaTeX;Tex;Overleaf;云计算;在线采编},
   Year = {2016} }




@article{
刘啸-391,
   Author = {刘啸},
   Title = {LaTeX排版书稿的编辑工作浅谈},
   Journal = {新媒体研究},
   Volume = {1},
   Number = {19},
   Pages = {64-66},
   Note = {页数: 3},
   Abstract = {本文结合La Te X排版软件的特点,总结了以La Te X软件排出的出版物的常见错误和需要特别注意的问题,并探索了责任编辑在latex排版书稿的编辑加工和校对过程中的一些工作方法和技巧。笔者作为La Te X排版软件多年的使用者和接触者,积累了丰富的经验或是教训。撰写这篇文章的目的 ,是想将这些经验或教训与同行分享,希望能对大家的工作有所帮助。},
   Keywords = {La Te X;编辑;校对},
   Year = {2015} }




@misc{
王立楠陈风云-400,
   Author = {王立楠 and 陈风云 and 刘玉娜},
   Title = {数字出版环境下科技期刊排版软件对比},
   Pages = {4},
   Abstract = {通过分析排版软件的适应环境,以及科技期刊的出版流程,调研了目前市面上比较流行的新版排版软件,如方正2008,方正飞翔,Latex及InDesign等。指出了各排版软件适应的排版环境,对比了各排版软件性能优劣。在数字出版背景下,方正飞翔及InDesign等显示出优异的数字出版性能。},
   Keywords = {数字出版;排版;方正;Latex;InDesign},
   Year = {2015} }


